<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>InsertUser</title>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
</head>
<body>
  <form  method="post" action="/insertCate" id="formCate">
    <div class="row g-3 align-items-center">
      <div class="col-auto">
        <label for="inputPassword6" class="col-form-label" >Category</label>
      </div>
      <div class="col-auto">
        <input type="Category" id="inputPassword6" class="form-control" aria-describedby="passwordHelpInline" name="Cate">
      </div>
      <div class="col-auto">
        <span id="passwordHelpInline" class="form-text">
          <button type="submit" class="btn btn-primary">submit</button>
        </span>
      </div>
    </div>

    <% for (var i = 0; i < cateData.length; i++) { %>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">ID</th>
            <th scope="col">Category</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row"><%= i %></th>
            <td><%= cateData[i]._id %></td>
            <td><%= cateData[i].Cate %></td>
            <td>
              <a href="/deleteCate/<%= cateData[i]._id %>"> <button type="button" class="btn btn-secondary">Delete</button></a>
            </td>
          </tr>
        </tbody>
      </table>
      <% } %>
  </form>
</body>
</html>